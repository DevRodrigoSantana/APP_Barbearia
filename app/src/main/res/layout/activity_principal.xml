<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".view.Principal">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:contentDescription="Botão com imagem de uma adição, nele você pode agendar um corte ver suas agendamentos e muitas funções!"
        android:scaleType="centerCrop"
        app:backgroundTint="@color/dark_primary"
        app:elevation="12dp"
        app:fabCustomSize="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/add_vector"
        app:tint="@android:color/white" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_agenda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:contentDescription="Botão com imagem de uma adição, nele você pode agendar um corte ver suas agendamentos e muitas funções!"
        android:scaleType="centerCrop"
        android:visibility="invisible"
        app:backgroundTint="@color/dark_primary"
        app:elevation="12dp"
        app:fabCustomSize="70dp"
        app:layout_constraintBottom_toTopOf="@+id/fab_corte"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_date_range_24"
        app:tint="@android:color/white" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_corte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:contentDescription="Botão com imagem de uma adição, nele você pode agendar um corte ver suas agendamentos e muitas funções!"
        android:scaleType="centerCrop"
        android:visibility="invisible"
        app:backgroundTint="@color/dark_primary"
        app:elevation="12dp"
        app:fabCustomSize="70dp"
        app:layout_constraintBottom_toTopOf="@+id/fab_add"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/cut_vector"
        app:tint="@android:color/white" />
</androidx.constraintlayout.widget.ConstraintLayout>